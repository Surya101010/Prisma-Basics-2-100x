generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}
model Users{
  id  Int  @id @default(autoincrement())
  username String @unique
  password String 
  age Int
  city String
  todos Todo[]
}

model Todo{
  id Int @id @default(autoincrement())
  title String
  description String
  done Boolean
  user_id Int 
  user Users @relation(fields: [user_id],references: [id])
}